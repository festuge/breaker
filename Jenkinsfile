pipeline{
    agent any
    stages{
        stage('1-kiss-menwo'){
            steps{
                echo "kisses with love"
            }
        }
        stage('2-hugs-for-menwo'){
            steps{
                echo "hugs that will amke you remember me for ever"
            }
        }
        stage('3-disc-space-check'){
            steps{
                sh 'lscpu'
            }
        }
        stage('4-cat_etc-passwd'){
            steps{
                sh 'cat /etc/passwd'
            }
        }
        stage('5-exucuting'){
            steps{
                sh 'sudo chmod +x freaking.sh'
            }
        }
        stage('6-run-file'){
            steps{
                sh 'freaking.sh'
            }
        }
    }
}
